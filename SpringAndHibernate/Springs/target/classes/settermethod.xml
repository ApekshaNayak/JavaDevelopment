<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="addr" class="com.project.settermethod.Address">
		<property name="addressLine1" value="8201 Memorial lane" />
		<property name="city" value="Plano" />
		<property name="state" value="Texas" />
		<property name="country" value="USA" />
	</bean>
	<bean id="employee" class="com.project.settermethod.Employee">
		<property name="id" value="101" />
		<property name="name" value="Apeksha" />
		<property name="address" ref="addr"></property>
	</bean>

</beans>